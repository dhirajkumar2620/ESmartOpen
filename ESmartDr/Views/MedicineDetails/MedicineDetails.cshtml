                            <p>Need to fill mandatory fields.</p>
                            <form>
                                <div class="row">
                                    <div class="col-md-4">
                                        <div class="form-group">
                                            <label class="form-control-label">Precription Name<span class="star">*</span></label>
                                            @Html.TextBoxFor(m => m.MedicineName, new { @id = "txtName", @class = "form-control", required = "required", @placeholder = "Precription Name" })
                                            @Html.ValidationMessageFor(model => model.MedicineName, "", new { @class = "text-danger" })


                                        </div>
                                    </div>
                                    <div class="col-md-4">
                                        <div class="form-group">
                                            <label class="form-control-label">Type<span class="star">*</span></label>
                                            @Html.TextBoxFor(m => m.MedicineType, new { @id = "txtName", @class = "form-control", required = "required", @placeholder = "Type" })
                                            @Html.ValidationMessageFor(model => model.MedicineType, "", new { @class = "text-danger" })


                                        </div>
                                    </div>
                                    <div class="col-md-4">
                                        <div class="form-group">
                                            <label class="form-control-label">Generic Name<span class="star">*</span></label>
                                            @Html.TextBoxFor(m => m.GenericName, new { @id = "txtName", @class = "form-control", required = "required", @placeholder = "Generic Name" })
                                            @Html.ValidationMessageFor(model => model.GenericName, "", new { @class = "text-danger" })


                                        </div>
                                    </div>
                                </div>
                                <div class="row">
                                    <div class="col-md-4">
                                        <div class="form-group">
                                            <label class="form-control-label">Company Name<span class="star">*</span></label>
                                            @Html.TextBoxFor(m => m.CompanyName, new { @id = "txtName", @class = "form-control", required = "required", @placeholder = "Company Name" })
                                            @Html.ValidationMessageFor(model => model.CompanyName, "", new { @class = "text-danger" })


                                        </div>
                                    </div>
                                    <div class="col-md-4">
                                        <div class="form-group">
                                            <label class="form-control-label">Range<span class="star">*</span></label>
                                            @Html.TextBoxFor(m => m.Range, new { @id = "txtName", @class = "form-control", required = "required", @placeholder = "Range" })
                                            @Html.ValidationMessageFor(model => model.Range, "", new { @class = "text-danger" })


                                        </div>
                                    </div>
                                    <div class="col-md-4">
                                        <div class="form-group">
                                            <label class="form-control-label">Other</label>
                                            @Html.TextBoxFor(m => m.Other, new { @id = "txtName", @class = "form-control", @placeholder = "Other" })
                                            @Html.ValidationMessageFor(model => model.Other, "", new { @class = "text-danger" })


                                        </div>
                                    </div>
                                </div>
                                <div class="row">
                                    <div class="col-md-12 text-right">
                                        <a href="~/Home/Apoinment"><input type="button" value="Back" class="btn btn_1 btn-primary"></a>
                                        <input type="submit" value="Save" class="btn btn_2 b33 btn-primary">
                                    </div>
                                </div>
                            </form>
                        </div>
                    </div>
                </div>
            </div>
        </div>
}
