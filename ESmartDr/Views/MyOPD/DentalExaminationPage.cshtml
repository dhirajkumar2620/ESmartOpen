                error: function (response) {
                    //alert(response.responseText);
                }
            });
        }
        else {
            document.getElementById(img).src = "";
            document.getElementById(id).classList.remove('btnRed');
            document.getElementById(id).classList.add('btnGreen');
            document.getElementById(img).src = '../img/' + imgName + '.png';
        }
    };

