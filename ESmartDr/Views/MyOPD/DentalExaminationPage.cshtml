        if (document.getElementById(id).classList.contains('btnGreen')) {
            debugger
            document.getElementById(img).src = "";
            document.getElementById(id).classList.remove('btnGreen');
            document.getElementById(id).classList.add('btnRed');
            var imgNumber = img.substring(3);
            var imgName = "P" + imgNumber;
            document.getElementById(img).src = '../img/' + imgName + '.png';

            $.ajax({
