<script type="text/javascript">
    function SaveDentalExaminationPage() {

        $(":radio").click(function () {
            var Selectedvalue = $(this).val();
            if (Selectedvalue = null || Selectedvalue == "") {
                alert("Selete color code");
                return;
            }
        });
        var Id = document.getElementById("hdnODId").value;
        //if (Amount = null || Amount == "") {
        //    alert("Enter amount");
        //    return;
        //}
       
        var Amount = document.getElementById("txtAmount").value;
        if (Amount = null || Amount == "") {
            alert("Enter amount");
            return;
        }
        var Notes = document.getElementById("txtNotes").value;
        if (Notes = null || Notes == "") {
            alert("Enter note");
            return;
        }
        var date1 = document.getElementById("txtDate").value
        if (date1 = null || date1 == "") {
            alert("Selete date");
            return;
        }
        var ToothProcedure = document.getElementById("ddlToothProcedure").value;
        if (ToothProcedure = null || ToothProcedure == "") {
            alert("Select tooth procedure");
            return;
        }
        var form = $('#frmObservationDetails')[0];

        var fromData = new FormData(form);
        var urllink = '@Url.Content("~/MyOPD/SavePage")'
        $.ajax({
            type: "post",
            url: urllink,
            data: fromData,
            processData: false,
            contentType: false,
            success: function (data) {
                $('#tblDentalExamination tbody').empty();
                var trHTML = '';
                $.each(data, function (i, item) {
                    trHTML += '<tr ><td hidden>' + item.Id + '</td><td>' + item.CreatedDate + '</td><td>' + item.ColorCode + '</td><td>' + item.ToothNumber + '</td><td>' + item.ToothProcedure + '</td><td>' + item.Amount + '</td><td>' + item.Notes + '</td><td><i class="fa fa-pencil" onclick=EditPICDetails(this) aria-hidden="true"></i>&nbsp;&nbsp;<i class="fa fa-times" onclick=DeleteRecord("' + item.Id + '") aria-hidden="true"></i></td></tr>';
                });
                $('#tblDentalExamination tbody').append(trHTML)
                $("#hdnODId").val('0');
                $("#txtAmount").val('');
                $("#txtNotes").val('');
                $("#txtDate").val('');
                $("#ddlToothProcedure").val('');
            },
            error: function (jqXHR, textStatus, errorThrown) {
                //console.log(errorThrown);
            }
        })
    }
    $(".fa-pencil").on("click", function () {

        var obj = $(this1).parents("tr")
        $("#hdnODId").val(obj.find('td')[0].innerHTML);
        $("#Id").val(obj.find('td')[0].innerHTML);
        $("#txtAmount").val(obj.find('td')[5].innerHTML);
        $("#txtNotes").val(obj.find('td')[6].innerHTML);
        $("#txtDate").val(obj.find('td')[1].innerHTML);
        $("#ddlToothProcedure").val(obj.find('td')[4].innerHTML);
