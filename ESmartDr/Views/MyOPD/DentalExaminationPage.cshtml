<script>




     function DeleteRecord(id) {
            if (confirm('Do you want to delete record ?')) {
                var urllink = '@Url.Content("~/MyOPD/DeleteDentalExamination")'
                $.ajax({
                    type: "post",
                    url: urllink,
                    data: '{Id: "' + id + '"}',
                    contentType: "application/json; charset=utf-8",
                    dataType: "json",
                    success: function (data) {
                        $('#tblDentalExamination tbody').empty();
                        var trHTML = '';
                        $.each(data, function (i, item) {
                            trHTML += '<tr ><td hidden>' + item.Id + '</td><td>' + item.CreatedDate + '</td><td>' + item.ColorCode + '</td><td>' + "T1" + '</td><td>' + item.ToothProcedure + '</td><td>' + item.Amount + '</td><td>' + item.Notes + '</td></td><td><i class="fa fa-pencil" onclick=EditDetails(this) aria-hidden="true"></i>&nbsp;&nbsp;<i class="fa fa-times" onclick=DeleteRecord("' + item.Id + '") aria-hidden="true"></i></td></tr>';
                        });
                        $('#tblDentalExamination tbody').append(trHTML)
                    }
                }

               )
            }
        };





